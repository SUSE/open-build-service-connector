{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./frontend_out",
    "rootDir": "src",
    "moduleResolution": "node",
    "lib": ["dom", "es2015"],
    "target": "es5"
  },
  "include": ["src/history-graph-common.ts", "src/frontend/*.ts"],
  "exclude": [
    "src/*.ts",
    "node_modules",
    ".vscode-test",
    "test-resources",
    "flycheck_*",
    "open-build-service"
  ]
}
